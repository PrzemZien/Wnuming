%zadanie 1
x = linspace(15,55);
w =  @(x) sin(x) .* x .^(3/2) -5.*(x-10);
wx = sin(x) .* x .^(3/2) -5.*(x-10); % musiałem tak zrobić jako, że plot nie apkceptuje @
plot(x,wx)
hold on
xd = linspace(20,50);
k = length(xd);

for i=1:k
    x0(i) = fzero(w,xd(i));
end

miejsca_zerowe = sort(x0)
n=1;
indeksy = find(20<miejsca_zerowe & miejsca_zerowe<50); 


for k=indeksy
    tab(n)=miejsca_zerowe(k); 
    n=n+1;
end

tab
plot(miejsca_zerowe,0,'r*')
